package com.zumbaapp.servlet.controller;

import jakarta.servlet.ServletException;
import jakarta.servlet.annotation.WebServlet;
import jakarta.servlet.http.HttpServlet;
import jakarta.servlet.http.HttpServletRequest;
import jakarta.servlet.http.HttpServletResponse;
import java.io.IOException;

import com.zumbaapp.servlet.model.Classes;

/**
 * Servlet implementation class Classes
 */
public class ClassesController extends HttpServlet {
	private static final long serialVersionUID = 1L;
       
    /**
     * @see HttpServlet#HttpServlet()
     */
    public ClassesController() {
        super();
        // TODO Auto-generated constructor stub
    }

	/**
	 * @see HttpServlet#service(HttpServletRequest request, HttpServletResponse response)
	 */
	protected void service(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		Classes classes = new Classes();
		request.getParameter("txtClassName");
		request.getParameter("txtSession");
		request.getParameter("txtClassDate");
		request.getParameter("txtClassStart");
		request.getParameter("txtClassEnd");
		request.getParameter("txtClassLocation");
	}

}
